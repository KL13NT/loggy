{
	"manifest_version": 2,
	"name": "Loggy",
	"version": "0.0.2",
	"description": "Gain insights on the time you spend online!",
	"permissions": [
		"storage",
		"unlimitedStorage",
		"tabs"
	],
	"options_ui": {
		"page": "options.html",
		"open_in_tab": true
	},
	"background": {
		"page": "background.html"
	},
	"icons": {
		"32": "icons/icon.svg",
		"48": "icons/icon.svg",
		"64": "icons/icon.svg",
		"96": "icons/icon.svg",
		"128": "icons/icon.svg"
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "{ae04bc71-2f28-4ace-8a96-470334639b65}",
			"strict_min_version":"70",
			"strict_max_version":"*"
		}
	}
}